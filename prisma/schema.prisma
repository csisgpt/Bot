generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Signal {
  id        String   @id @default(uuid())
  symbol    String
  interval  String
  type      String
  time      DateTime
  price     Float
  emaFast   Float
  emaSlow   Float
  rsi       Float
  createdAt DateTime @default(now())

  @@index([symbol, interval, time])
}
